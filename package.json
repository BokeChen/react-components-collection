  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@types/lodash": "^4.14.168",
    "@types/redux-logger": "^3.0.8",
    "antd": "^4.15.1",
    "cra-template-typescript": "1.1.2",
    "easy-peasy": "^5.0.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
